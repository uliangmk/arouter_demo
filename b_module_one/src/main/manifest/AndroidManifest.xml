<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blued.b_module_one">

    <!--此module单独打包apk时使用此manifest-->
    <application
        android:name=".OneApplication"
        android:allowBackup="true"
        android:icon="@mipmap/c_module_main_ic_launcher"
        android:label="单独one"
        android:theme="@style/x_module_common_AppTheme">
        <activity android:name=".OneActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.blued.c_module_main.MainActivity"/>
    </application>

</manifest>
